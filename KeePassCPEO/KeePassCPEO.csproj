DEL /s /f /q "$(TargetDir)$(TargetName)\*.*"
RMDIR /s /q "$(TargetDir)$(TargetName)

COPY "$(TargetDir)$(TargetName).plgx" "%25KPPluginsDir%25$(TargetName).plgx"

)</PostBuildEvent>
